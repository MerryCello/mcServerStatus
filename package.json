{
  "name": "mc-server-status",
  "version": "1.2.2",
  "homepage": "https://merrycello.github.io/mcServerStatus",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "npx update-browserslist-db@latest -y && react-scripts build",
    "test": "jest --watchAll=false",
    "coverage": "jest --coverage --silent",
    "eject": "react-scripts eject",
    "pre-deploy": "run-p test lint tsc && npm run build",
    "deploy": "sh ./scripts/deploy.sh",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 0",
    "lint:fix": "npm run lint -- --fix",
    "pre-commit": "run-p test lint tsc",
    "pre-push": "run-p test lint tsc",
    "tsc": "tsc"
  },
  "dependencies": {
    "firebase": "~11.2.0",
    "html-react-parser": "^3.0.4",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-movable": "^3.4.0",
    "react-router-dom": "^6.4.2",
    "react-scripts": "^5.0.1",
    "superagent": "^8.0.0",
    "typescript": "^4.9.5",
    "use-long-press": "^3.2.0",
    "uuid": "^9.0.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.23.3",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/firebase": "^3.2.1",
    "@types/jest": "^27.5.2",
    "@types/lodash": "^4.17.14",
    "@types/node": "^16.18.41",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/react-router-dom": "^5.3.3",
    "@types/superagent": "^4.1.18",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "babel-jest": "^29.7.0",
    "eslint": "8.41.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^6.3.0",
    "husky": "^9.0.7",
    "npm-run-all": "^4.1.5"
  },
  "overrides": {
    "@adobe/css-tools": "~>4.3.1",
    "@babel/traverse": "~>7.23.2",
    "cookiejar": "~>2.1.4",
    "follow-redirects": "~>1.15.4",
    "json5": "~>1.0.2",
    "minimatch": "~>3.0.5",
    "nth-check": "~>2.0.1",
    "postcss": "~>8.4.31",
    "qs": "~>6.9.7",
    "react-devtools-core": "~>4.28.4",
    "semver": "~>7.5.2",
    "tough-cookie": "~>4.1.3",
    "webpack": "~>5.96.1",
    "word-wrap": "~>1.2.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit",
      "pre-push": "npm run pre-push"
    }
  }
}